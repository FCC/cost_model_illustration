 
#camtract::center [zoom>8] {
    line-color: #8F6CD7;
    line-dasharray: 1,1;
    [zoom=9] {line-width: 0.5;}
    [zoom=10] {line-width: 1.0;}
    [zoom=11] {line-width: 1.5;}
    [zoom=12] {line-width: 1.5;}
  } 
    
#camtract [zoom>8] {
  polygon-fill: rgba(0,0,0,0);
  line-color:lightgray;
  line-opacity: 0.25;
  [zoom=9] {line-width:1.5;}
  [zoom=10] {line-width:2.5;}
  [zoom=11] {line-width:6.5;}
  [zoom=12] {line-width:8.5;}
} 